<div class="col" id="<%= dom_id post %>">
  <div>
    <p>
      <strong>Author:</strong>
      <%= "#{post.user.id} - #{post.user.email}" %>
      <br>
      <%= post.user.full_name %>
    </p>

    <p>
      <strong>Title:</strong>
      <%= post.title %>
    </p>

    <p>
      <strong>Content:</strong>
      <%= post.content %>
    </p>
  </div>
</div>
